<?xml version="1.0"?>
<block>
    <name>FSK Demodulator (RTTY)</name>
    <key>shinysdr_RTTYFSKDemodulator</key>
    <category>Modulators</category>
    <import>import shinysdr.plugins.rtty</import>
    <make>shinysdr.plugins.rtty.RTTYFSKDemodulator(input_rate=$input_rate, baud=$baud)</make>
    <param>
        <name>Input Sample Rate</name>
        <key>input_rate</key>
        <value>samp_rate</value>
        <type>real</type>
    </param>
    <param>
        <name>Baud Rate</name>
        <key>baud</key>
        <value>45.45</value>
        <type>real</type>
    </param>
    <sink>
        <name>in</name>
        <type>complex</type>
    </sink>
    <source>
        <name>out</name>
        <type>float</type>
    </source>
</block>